{
  "name": "tacklebox",
  "version": "1.0.53",
  "private": true,
  "scripts": {
    "auditfix": "yarn yarn-audit-fix",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel": "vercel dev",
    "test": "jest --verbose",
    "test:coverage": "yarn test --coverage",
    "checks": "yarn cspell ; yarn lint ; yarn jscpd ; yarn test",
    "cspell": "cspell -u --gitignore .",
    "jscpd": "jscpd .",
    "lint": "yarn lint:js ; yarn lint:prettier ; yarn lint:style",
    "lint:js": "eslint -f compact .",
    "lint:prettier": "prettier --check .",
    "lint:style": "stylelint -f compact \"**/*.css\"",
    "lintfix": "yarn lint:js --fix ; prettier --write --list-different . ; yarn lint:style --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@vercel/analytics": "^1.0.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "lodash": "^4.17.21",
    "next": "13.4.5",
    "next-pwa": "^5.6.0",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "vercel": "^30.2.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "cspell": "^6.31.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jscpd": "^3.5.9",
    "msw": "^1.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
    "yarn-audit-fix": "^9.3.12"
  }
